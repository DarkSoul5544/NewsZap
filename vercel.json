{
    "version": 2,
    "builds": [
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/build"
        }
      }
    ],
    "routes": [
      {
        "src": "backend/(.*)",
        "dest": "backend/server.js"
      },
      {
        "src": "(.*)",
        "dest": "$1"
      }
    ]
  }
  